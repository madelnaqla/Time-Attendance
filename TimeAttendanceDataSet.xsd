                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CRD.CRD_NO, CRD.CRD_NAME, CRD.CRD_JOB, CRD.CRD_DEPARTMENT, CRD.COMPANY, CRD.LOCATION, TRAN_IN.TRN_TIME AS ENT_TIME, 
                         TRA.TRN_MACHINE, TRA.TRN_PLACE
